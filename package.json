{
  "name": "angular-es6-webpack-template",
  "version": "0.0.1",
  "description": "A template that can be used to set up a project when writing (large) applications in angularJS 1.5 or higher.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-dev": "webpack -d --display-modules --progress",
    "build-prod": "webpack -p --display-modules --progress --config webpack.prod.config.js",
    "start": "webpack-dev-server -d  --progress --open --inline --port 9090 --content-base www/"
  },
  "author": "Marcel van de Kamp",
  "license": "ISC",
  "dependencies": {
    "angular": "^1.5.8",
    "angular-ui-router": "^1.0.0-beta.1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-runtime": "^6.11.6",
    "clean-webpack-plugin": "^0.1.10",
    "ng-annotate-webpack-plugin": "^0.1.3",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.15.0"
  }
}